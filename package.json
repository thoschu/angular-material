{
  "name": "angular-material",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:development": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "http-server:browser": "npx http-server ./dist/angular-material/browser --cors true --log-ip true -c-1",
    "ngrok": "npx ngrok http 4200",
    "rimraf:dist": "npx rimraf dist",
    "rimraf:node_modules": "npx rimraf node_modules",
    "clean:all": "npx concurrently 'npm run rimraf:dist' 'npm run rimraf:node_modules'",
    "_tailwindcss": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "server": "npx bun index.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/cdk": "^17.3.1",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/material": "^17.3.1",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@angular/service-worker": "^17.3.1",
    "@jsverse/transloco": "^7.1.0",
    "@ngrx/effects": "^17.1.1",
    "@ngrx/entity": "^17.1.1",
    "@ngrx/router-store": "^17.1.1",
    "@ngrx/store": "^17.1.1",
    "@ngrx/store-devtools": "^17.1.1",
    "animate.css": "^4.1.1",
    "cookieconsent": "^3.1.1",
    "highlight.js": "^11.9.0",
    "ngx-cookieconsent": "^6.0.0",
    "ngx-highlightjs": "^11.0.0-beta.1",
    "ol": "^9.1.0",
    "proj4": "^2.11.0",
    "ramda": "^0.29.1",
    "rxjs": "~7.8.0",
    "store": "^2.0.12",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.3",
    "@angular/cli": "^17.1.3",
    "@angular/compiler-cli": "^17.1.0",
    "@types/bun": "latest",
    "@types/jasmine": "~5.1.0",
    "@types/proj4": "^2.5.5",
    "@types/ramda": "^0.29.11",
    "autoprefixer": "^10.4.19",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
